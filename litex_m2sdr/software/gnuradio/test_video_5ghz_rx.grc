- name: video_sdl_sink_0
  id: video_sdl_sink
  parameters:
    affinity: ''
    alias: ''
    comment: ''
    display_height: 240 * 2
    display_width: 360 * 2
    fps: '0'
    height: '240'
    num_channels: '1'
    type: short
    width: '360'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1232, 316.0]
    rotation: 0
    state: enabled

